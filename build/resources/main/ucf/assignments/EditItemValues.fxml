<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="175.0" prefWidth="426.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.EditItemValuesController">
   <children>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="426.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#closeButtonClicked" text="Close" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TextField fx:id="newItemNameField" layoutX="14.0" layoutY="38.0" prefHeight="25.0" prefWidth="400.0" promptText="New Item Name" />
      <TextField fx:id="newItemValueField" layoutX="14.0" layoutY="75.0" prefHeight="25.0" prefWidth="400.0" promptText="New Item Value" />
      <TextField fx:id="newItemSerialField" layoutX="14.0" layoutY="110.0" prefHeight="25.0" prefWidth="400.0" promptText="New Item Serial Number" />
      <Button layoutX="98.0" layoutY="140.0" mnemonicParsing="false" onAction="#saveChangesButtonClicked" prefHeight="31.0" prefWidth="235.0" text="Save Changes" />
   </children>
</AnchorPane>
